<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#009688"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="185dp"
                android:height="53dp"
                />
            <stroke
                android:width="2dp"
                android:color="#FFFFFF"
                />
        </shape>
    </item>

    <item
        android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#00CCB9"
                />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp"
                />
            <size
                android:width="185dp"
                android:height="53dp"
                />
        </shape>
    </item>
</selector>